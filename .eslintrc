{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "no-console": [
            "error",
            { "allow": ["warn", "error", "log"] }
        ],
        "no-unused-vars": "off"
    }
}
